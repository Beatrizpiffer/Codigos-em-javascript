<body>
   <h1>Seção 04 - Aula 04</h1>
       
    <script>
        var num1 = 42; num2 = 19, nome = 'Geek';
        //var num2 = 19;
        console.log(num1, num2, nome);

        // soma
        var res = num1 + num2;
        console.log("O resultado é:  " + res);
        console.log(nome);

        // CUIDADO! Dica Geek!
        var num3 = "39";
        res = num1 + num3;
        console.log(res);
        
        // Subtração 
        res = num1 - num2;
        console.log(res);

        //Multiplicação
        res = num1 * num2;
        console.log(res);

        //Divisão
        res = num1 / 2;
        console.log(res);

    </script>
